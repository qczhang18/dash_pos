<h1> Category <%= @category.name %> </h1>
<h3> Included are </h3>

  <div class="row">
    <% @items.each do |item| %>
      <div class="col-sm-4">
        <div class="panel panel-primary">
          <div class="panel-heading"> <%= item.name %> </div>
          <div class="panel-body">
            <%#= link_to item_path(item) %>
            <img src="http://www.placecage.com/150/150" class="img-responsive" style="width:100%" alt="Image">
          </div>
        </div>
      </div>
    <% end %>
  </div>

